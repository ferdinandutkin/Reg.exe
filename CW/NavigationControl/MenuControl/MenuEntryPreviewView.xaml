<rxui:ReactiveUserControl x:Class="CWRegexTester.MenuEntryPreviewView"
					  x:TypeArguments="local1:MenuEntryPreview"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CWRegexTester"
				 xmlns:rxui="http://reactiveui.net" xmlns:local1="clr-namespace:CWRegexTester"
             mc:Ignorable="d" 
					HorizontalContentAlignment="Stretch"	   
             d:DesignHeight="500" d:DesignWidth="800">
	<Grid  HorizontalAlignment="Stretch">

        <Border Background="{StaticResource SlateBrush}" CornerRadius="12" 
                HorizontalAlignment="Stretch" BorderThickness="0">
            <TextBlock x:Name="Title" 
                       TextWrapping="Wrap" 

                       Style="{StaticResource MenuButtonsStyle}"/>
		</Border>
		
	</Grid>
</rxui:ReactiveUserControl>
